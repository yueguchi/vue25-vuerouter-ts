<?xml version='1.0' encoding='utf-8'?>
<widget id="com.hogefuga.testest" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>vue25-test</name>
    <description>
      Vue2 + vuex + vue-router + typescript + Onsen UI + Cordova によるハイブリッドアプリ
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
      Vue.js + Onsen UI + Cordova teams
    </author>
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#297afb" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="72" src="res/icon/ios/icon-72.png" width="72" />
        <icon height="144" src="res/icon/ios/icon-72@2x.png" width="144" />
        <icon height="76" src="res/icon/ios/icon-76.png" width="76" />
        <icon height="152" src="res/icon/ios/icon-76@2x.png" width="152" />
    </platform>
    <splash height="1536" platform="ios" src="www/static/img/splash@2x.png" width="2048" />
    <engine name="android" spec="^6.3.0" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-wkwebview-engine" spec="^1.1.3" />
</widget>
